{
  "name": "hugo",
  "externals": [
    { "type": "SYMBOL", "name": "_start_tag_name" },
    { "type": "SYMBOL", "name": "_script_start_tag_name" },
    { "type": "SYMBOL", "name": "_style_start_tag_name" },
    { "type": "SYMBOL", "name": "_end_tag_name" },
    { "type": "SYMBOL", "name": "erroneous_end_tag_name" },
    { "type": "SYMBOL", "name": "raw_text" },
    { "type": "SYMBOL", "name": "comment" },
    { "type": "SYMBOL", "name": "HUGO_DOUBLE_BRACE_OPEN" },
    { "type": "SYMBOL", "name": "HUGO_DOUBLE_BRACE_CLOSE" }
  ],
  "extras": [{ "type": "PATTERN", "value": "/\\s+/" }],
  "conflicts": [],
  "inline": [],
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        { "type": "REPEAT", "rule": { "type": "SYMBOL", "name": "_node" } }
      ]
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        { "type": "SYMBOL", "name": "element" },
        { "type": "SYMBOL", "name": "comment" },
        { "type": "SYMBOL", "name": "raw_text" },
        { "type": "SYMBOL", "name": "HUGO_DOUBLE_BRACE_OPEN" },
        { "type": "SYMBOL", "name": "HUGO_DOUBLE_BRACE_CLOSE" }
      ]
    },
    "element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            { "type": "SYMBOL", "name": "start_tag" },
            { "type": "REPEAT", "rule": { "type": "SYMBOL", "name": "_node" } },
            {
              "type": "CHOICE",
              "members": [
                { "type": "SYMBOL", "name": "end_tag" },
                { "type": "SYMBOL", "name": "_implicit_end_tag" }
              ]
            }
          ]
        },
        { "type": "SYMBOL", "name": "self_closing_tag" }
      ]
    },
    "start_tag": {
      "type": "SEQ",
      "members": [
        { "type": "PATTERN", "value": "<" },
        { "type": "SYMBOL", "name": "_start_tag_name" },
        { "type": "REPEAT", "rule": { "type": "SYMBOL", "name": "attribute" } },
        { "type": "PATTERN", "value": ">" }
      ]
    },
    "end_tag": {
      "type": "SEQ",
      "members": [
        { "type": "PATTERN", "value": "</" },
        { "type": "SYMBOL", "name": "_end_tag_name" },
        { "type": "PATTERN", "value": ">" }
      ]
    },
    "self_closing_tag": {
      "type": "SEQ",
      "members": [
        { "type": "PATTERN", "value": "<" },
        { "type": "SYMBOL", "name": "_start_tag_name" },
        { "type": "REPEAT", "rule": { "type": "SYMBOL", "name": "attribute" } },
        { "type": "PATTERN", "value": "/>" }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        { "type": "SYMBOL", "name": "attribute_name" },
        {
          "type": "OPTIONAL",
          "rule": {
            "type": "SEQ",
            "members": [
              { "type": "PATTERN", "value": "=" },
              {
                "type": "CHOICE",
                "members": [
                  { "type": "SYMBOL", "name": "attribute_value" },
                  { "type": "SYMBOL", "name": "quoted_attribute_value" }
                ]
              }
            ]
          }
        }
      ]
    },
    "attribute_name": { "type": "PATTERN", "value": "[^<>=\\s]+" },
    "attribute_value": { "type": "PATTERN", "value": "[^<>=\\s]+" },
    "quoted_attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            { "type": "PATTERN", "value": "'" },
            { "type": "OPTIONAL", "rule": { "type": "PATTERN", "value": "[^']+" } },
            { "type": "PATTERN", "value": "'" }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            { "type": "PATTERN", "value": "\"" },
            { "type": "OPTIONAL", "rule": { "type": "PATTERN", "value": "[^\"]+" } },
            { "type": "PATTERN", "value": "\"" }
          ]
        }
      ]
    }
  }
}
